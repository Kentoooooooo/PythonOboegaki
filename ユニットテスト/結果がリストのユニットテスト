import unittest


def is_even(num):
    return num % 2 == 0


class TestEven(unittest.TestCase):

    def test_is_even_2(self):                  â† å®Ÿéš›ã«é–¢æ•°ã«å…¥åŠ›ã™ã‚‹å€¤ã‚’ãƒ†ã‚¹ãƒˆé–¢æ•°åã«å…¥ã‚Œã‚‹ã®ã‹ã‚‚
        """2ãŒå¶æ•°ã®ãƒ†ã‚¹ãƒˆ."""
